{
  "name": "rrbit",
  "version": "1.0.0",
  "description": "an immutable list lib using rrb trees",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "fantasy-land": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-polyfill": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "benchmark": "^2.1.2",
    "chai": "^3.5.0",
    "fantasy-check": "^0.3.2",
    "immutable": "^3.8.1",
    "immutable-array-methods": "^1.5.0",
    "mocha": "^3.1.2",
    "mori": "^0.3.2",
    "rollup": "^0.36.3",
    "seamless-immutable": "^6.3.0",
    "uglify-js": "^2.7.4"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
